{
  "extends": "airbnb/base",
  "globals": {
    "angular": false,
    "beforeEach": false,
    "describe": false,
    "expect": false,
    "it": false
  },
  "plugins": ["angular"],
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "id-length": 0,
    "no-param-reassign": 0,
    "object-shorthand": [2, "methods"],
    "radix": 0,
  }
}
